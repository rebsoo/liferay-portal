</tr>

<tr>
	<td>CHECKBOX_PREDEFINED_LIST</td>
	<td>//li[contains(@class,'dropdown-item')]//input[contains(@type,'checkbox')]//following-sibling::span[contains(.,'${key_fieldName}')]</td>
	<td></td>
</tr>

<!--RADIO_BUTTON-->
<tr>
	<td>RADIO_OPTION_LABEL</td>
	<td>//p[contains(text(),'${key_fieldName}')]/following-sibling::div//span[contains(.,'${key_radioOption}')]</td>
	<td></td>
</tr>

<!--DATE-->
<tr>
	<td>DATE_FIELD</td>
	<td>//p[contains(text(),'${key_fieldName}')]/..//input[1]</td>
	<td></td>
</tr>

<tr>
	<td>TEXT_BOX_FIELD</td>
	<td>//p[contains(text(),'${key_fieldName}')]/..//input[1]</td>
	<td></td>
</tr>

<!--OBJECT_FIELD-->
<tr>
	<td>OBJECT_FIELD_NAME</td>
	<td>//div[contains(@class,'sidebar-body')]//span[contains(text(),'${key_fieldName}')]</td>
	<td></td>
</tr>

<tr>
	<td>OBJECT_FIELD_PLUS</td>
	<td>//div[contains(@class,'custom-object-sidebar-header')]//*[contains(@class,'lexicon-icon-plus')]</td>
<!--FORM VALUES PREDEFINED-->

<tr>
	<td>PREDEFINED_VALUE_AT_FORM</td>
	<td>//p[contains(text(),'${key_fieldName}')]/..//input[contains(@value,'${key_fieldValue}')]</td>
	<td></td>
</tr>

<tr>
	<td>OBJECT_FIELD_SEARCH_BUTTON</td>
	<td>//div[contains(@class,'custom-object-sidebar-header')]//*[contains(@class,'lexicon-icon-search')]</td>
	<td></td>
</tr>

<tr>
	<td>OBJECT_FIELD_SEARCH_BOX</td>
	<td>//div[contains(@class,'custom-object-sidebar-header')]//input</td>
	<td></td>
</tr>
	<td>PREDEFINED_VALUE_AT_FORM_SELECTLIST</td>
	<td>//p[contains(text(),'${key_fieldName}')]/..//div[contains(@class,'option-selected') and contains(text(),'${key_fieldValue}')]</td>
	<td></td>
</tr>



<tr>
	<td>OBJECT_FIELD_SEARCH_RESULT</td>
	<td>//div[contains(@class,'custom-object-field')]//span[contains(text(),'${key_fieldName}')]</td>
	<td></td>
</tr>

<tr>
	<td>OBJECT_FIELD_BUTTON_DROPDOWN</td>
	<td>//button[@class='dropdown-item' and contains(text(),'${key_fieldName}')]</td>
	<td></td>
</tr>

